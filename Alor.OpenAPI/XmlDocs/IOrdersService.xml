          <listheader>
            <description>Направление сделки:</description>
          </listheader>
          <item>
            <term>buy</term>
            <description />Купля</item>
          <item>
            <term>sell</term>
            <description />Продажа</item>
        </list>
      </param>
      <param name="quantity">Количество (лоты)</param>
      <param name="symbol">Тикер (Код финансового инструмента)</param>
