          <listheader>
            <description>Биржа:</description>
          </listheader>
          <item>
            <term>MOEX</term>
            <description />Московская биржа</item>
          <item>
            <term>SPBX</term>
            <description />СПБ Биржа</item>
        </list>
      </param>
      <param name="instrumentGroup">
        <description>Код режима торгов (Борд):</description>
